{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "col"
  },
  "spec": {
    "containers": [
      {
        "name": "the_name",
        "image": "us.gcr.io/direct-disk-101619/docker",
        "workingDir": "/home/beamuser/run",
        "command": [ "python", "main.py"],
        "args": ["1000000000"],
        "resources": {
            "limits": {
                "cpu": "700m"
            }
        }
      }
    ],
    "restartPolicy": "Never"
  }
}

