{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "THE_NAME"
  },
  "spec": {
    "containers": [
      {
        "name": "the_name",
        "image": "us.gcr.io/direct-disk-101619/DOCKER",
        "workingDir": "/home/beamuser/run",
        "command": [ "python", "main.py"],
        "args": ["config_run.json", "25", "2000000000", "32", "2711", "2711"],
        "resources": {
            "limits": {
                "cpu": "700m"
            }
        }
      }
    ],
    "restartPolicy": "Never"
  }
}
